<div class="page-header">
	<h1><i class="icon-book"> </i>Protocols</h1>
</div>
<p>
	<table class="table table-bordered table-striped">
		<thead>
			<tr>
				<th>Protocol</th>
				<th>Author</th>
				<th>Edit</th>
			</tr>
		</thead>
		<tbody>		
			<% @protocols.each do |p| %>
				<tr>
					<td><%= link_to p.name, admin_protocol_path(p) %></td>
					<td><%= p.user.fullname %></td>
					<td><%= link_to 'Edit', edit_admin_protocol_path(p) %>
				</tr>
			<% end %>
		</tbody>
	</table>
</p>