<div class="page-header">
	<h1><i class="icon-resize-small"> </i>Sequencing</h1>
</div>
<p>
	<table class="table table-bordered table-striped">
		<thead>
			<tr>
				<th>#</th>
				<th>Plates</th>
				<th>Date</th>
			</tr>
		</thead>
		<tbody>		
			<% @sequencings.each do |seq| %>
				<tr>
					<td><%= link_to seq.id, sequencing_path(seq) %></td>
					<td>
						<% seq.sequencing_growth_plates.each do |plate| %>
							<%= plate.name %>; 
						<% end %>
					</td>
					<td><%= seq.created_at %></td>	
				</tr>
			<% end %>
		</tbody>
	</table>
</p>
