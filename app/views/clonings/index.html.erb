<div class="page-header">
	<h1><i class="icon-repeat"> </i>Clonings</h1>
</div>
<p>
	<table class="table table-bordered table-striped">
		<thead>
			<tr>
				<th>#</th>
				<th>Plate</th>
				<th>Date</th>
			</tr>
		</thead>
		<tbody>		
			<% @clonings.each do |c| %>
				<tr>
					<td><%= link_to c.id, cloning_path(c) %></td>
					<td><%= c.growth_plate.name %></td>
					<td><%= c.created_at %></td>	
				</tr>
			<% end %>
		</tbody>
	</table>
</p>
